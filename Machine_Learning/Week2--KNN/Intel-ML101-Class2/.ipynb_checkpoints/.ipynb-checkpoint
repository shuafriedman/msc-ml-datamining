    }
   ],
   "source": [
    "\n",
    "# Initialization\n",
    "W = 0\n",
    "b = 0\n",
    "L = 0.1 # The learning Rate\n",
    "epochs = 50  # The number of iterations to perform gradient descent\n",
    "\n",
    "n = float(len(X)) # Number of elements in X\n",
    "\n",
    "b_ = []\n",
    "W_ = []\n",
    "# Performing Gradient Descent\n",
    "for i in range(epochs):\n",
    "    Y_pred = W*X + b  # The current predicted value of Y\n",
    "    D_W = (-2/n) * sum(X * (Y - Y_pred))  # Derivative wrt W\n",
    "    D_b = (-2/n) * sum(Y - Y_pred)  # Derivative wrt b\n",
    "    W = W - L * D_W  # Update W\n",
    "    b = b - L * D_b  # Update b\n",
    "    W_.append(W)\n",
    "    b_.append(b)\n",
    "print(W,b)\n",
    "plt.plot(W_)\n",
    "plt.plot(b_)"
   ]
  },

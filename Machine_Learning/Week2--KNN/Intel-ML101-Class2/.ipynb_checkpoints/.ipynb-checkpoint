     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 432x288 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Stochastic (minibatch)\n",
    "# we use much less samples per epoch we can increse the learning rate = less computations\n",
    "#################\n",
    "# Initialization\n",
    "W = 0\n",
    "b = 0\n",
    "L = 3 # The learning Rate\n",
    "epochs = 500  # The number of iterations to perform gradient descent\n",
    "minibatch_size = 16\n",
    "\n",
    "n = float(len(X)) # Number of elements in X\n",
    "\n",
    "b_ = []\n",
    "W_ = []\n",
    "# Performing Gradient Descent\n",
    "for i in range(epochs):\n",
    "    #choose a few examples from X in each epoch\n",
    "    idx = np.random.randint(len(X),size=minibatch_size) \n",
    "    Y_pred = W*X[idx] + b  # The current predicted value of Y\n",
    "    D_W = (-2/n) * np.sum(X[idx] * (Y[idx] - Y_pred))  # Derivative wrt W\n",
    "    D_b = (-2/n) * np.sum(Y[idx] - Y_pred)  # Derivative wrt b\n",
    "    W = W - L * D_W  # Update W\n",
    "    b = b - L * D_b  # Update b\n",
    "    W_.append(W)\n",
    "    b_.append(b)\n",
    "print(W,b)\n",
    "plt.plot(W_)\n",
    "plt.plot(b_)"
   ]
  },

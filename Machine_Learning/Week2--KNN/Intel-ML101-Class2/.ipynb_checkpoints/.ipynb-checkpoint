     ]
    }
   ],
   "source": [
    "#try the closed form formula\n",
    "X_=X[:1000]\n",
    "y_=Y[:1000]\n",
    "y_=y_.reshape(1,-1)\n",
    "X_=X_.reshape(1,-1)\n",
    "\n",
    "w=np.matmul(X_.T,X_)\n",
    "w = np.linalg.pinv(w)\n",
    "\n",
    "w = np.matmul(w,X_.T)\n",
    "\n",
    "w = np.matmul(y_,w)\n",
    "print(w)"
   ]
  },
  {
   "cell_type": "code",

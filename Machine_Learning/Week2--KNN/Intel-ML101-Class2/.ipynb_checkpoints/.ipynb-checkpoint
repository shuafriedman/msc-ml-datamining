    "\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "#import seaborn as sns\n",
    "import matplotlib\n",
    "import matplotlib.pyplot as plt\n",
    "%matplotlib inline\n",
    "import matplotlib.animation as animation\n",
    "\n",
    "table = pd.read_csv('C:\\\\WinPython-3.5\\\\notebooks\\\\docs\\\\weight-height.csv')\n",
    "height = table['Height'][table['Gender'] == 'Male'].tolist()\n",
    "weight = table['Weight'][table['Gender'] == 'Male'].tolist()\n",
    "\n",
    "X = (weight-np.mean(weight))/np.std(weight)\n",
    "Y = np.array(height)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Explicit minimization\n",
    "\n",
    "## phase_1 = np.matmul(X.reshape(-1,1),X.reshape(1,-1))\n",
    "# Use pseudo-nverse to avoid singularities\n",
    "\n",
    "## phase_2 = np.linalg.pinv(phase_1)\n",
    "## phase_3 = np.matmul(phase_2,X.reshape(-1,1))\n",
    "\n",
    "# W_opt = np.matmul(Y.reshape(1,-1),phase_3)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [
